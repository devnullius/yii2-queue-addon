{
    "name": "devnullius/yii2-queue-addon",
    "description": "Addon for yii2-queue original extension",
    "type": "yii2-extension",
    "minimum-stability": "stable",
    "keywords": [
        "yii2",
        "extension",
        "queue",
        "addon"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "devnullius",
            "email": "aram.ds@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0 < 8.4.*",
        "yiisoft/yii2": "~2.0.37",
        "yiisoft/yii2-queue": "^2.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "devnullius\\queue\\addon\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "process-timeout": 1800
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
